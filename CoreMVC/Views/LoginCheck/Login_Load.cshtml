@model CoreMVC.Models.Employee
<form asp-action="Login_Click" class="form-horizontal" method="get">
    <div class="form-group">
        <label class="control-label">Employee id</label>
        <input asp-for="eid" class="form-control" id="t1" />
    </div>
    <div class="form-group">
        <label class="control-label">Employee Name</label>
        <input asp-for="ename" class="form-control" id="t2" />
    </div>
    <div class="form-group">
        <input type="submit" value="Insert" class="btn btn-success" />
    </div>
</form>
@{
    if (TempData["msg"] != null)
    {
        <script>
            alert('@TempData["msg"]');
        </script>
    }
}